<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="libASE" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="Release DLL">
				<Option output="..\..\bin\libASE.dll" prefix_auto="0" extension_auto="0" />
				<Option object_output="..\..\lib\obj\release dll" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option createDefFile="1" />
				<Option createStaticLib="1" />
				<Compiler>
					<Add option="-Os" />
					<Add option="-O2" />
					<Add option="-DBUILDING_DLL=1 -DLIBASE_EXPORTS=1 -D_MSC_VER=1" />
				</Compiler>
				<Linker>
					<Add option="-s" />
				</Linker>
			</Target>
			<Target title="Debug DLL">
				<Option output="..\..\bin\libASEd.dll" prefix_auto="0" extension_auto="0" />
				<Option object_output="..\..\lib\obj\debug dll" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option createDefFile="1" />
				<Option createStaticLib="1" />
				<Compiler>
					<Add option="-pedantic" />
					<Add option="-Wall" />
					<Add option="-pg" />
					<Add option="-g" />
					<Add option="-DBUILDING_DLL=1 -DLIBASE_EXPORTS=1 -D_MSC_VER=1" />
				</Compiler>
				<Linker>
					<Add option="-pg -lgmon" />
				</Linker>
			</Target>
			<Target title="Release">
				<Option output="..\..\lib\libASE.a" prefix_auto="0" extension_auto="0" />
				<Option working_dir="" />
				<Option object_output="..\..\lib\obj\release" />
				<Option type="2" />
				<Option compiler="gcc" />
				<Option createDefFile="1" />
				<Compiler>
					<Add option="-Os" />
					<Add option="-O2" />
				</Compiler>
				<Linker>
					<Add option="-s" />
				</Linker>
			</Target>
			<Target title="Profile">
				<Option output="..\..\lib\libASEp.a" prefix_auto="0" extension_auto="0" />
				<Option working_dir="" />
				<Option object_output="..\..\lib\obj\profile" />
				<Option type="2" />
				<Option compiler="gcc" />
				<Option createDefFile="1" />
				<Compiler>
					<Add option="-Os" />
					<Add option="-O2" />
					<Add option="-pg" />
				</Compiler>
				<Linker>
					<Add option="-s" />
					<Add option="-pg -lgmon" />
				</Linker>
			</Target>
			<Target title="Debug">
				<Option output="..\..\lib\libASEd.a" prefix_auto="0" extension_auto="0" />
				<Option working_dir="" />
				<Option object_output="..\..\lib\obj\debug" />
				<Option type="2" />
				<Option compiler="gcc" />
				<Option createDefFile="1" />
				<Compiler>
					<Add option="-Wall" />
					<Add option="-pg" />
					<Add option="-g" />
				</Compiler>
				<Linker>
					<Add option="-pg -lgmon" />
				</Linker>
			</Target>
		</Build>
		<VirtualTargets>
			<Add alias="All DLL" targets="Release DLL;Debug DLL;" />
			<Add alias="All Static" targets="Release;Profile;Debug;" />
		</VirtualTargets>
		<Compiler>
			<Add option="-D_WIN32=1" />
		</Compiler>
		<Linker>
			<Add option="--no-export-all-symbols --add-stdcall-alias --output-def liblibASE.def" />
		</Linker>
		<Unit filename="ASElexer.c">
			<Option compilerVar="CPP" />
		</Unit>
		<Unit filename="libASE.h" />
		<Unit filename="loader.c">
			<Option compilerVar="CPP" />
		</Unit>
		<Unit filename="symbols.h" />
		<Extensions>
			<code_completion />
			<envvars />
		</Extensions>
	</Project>
