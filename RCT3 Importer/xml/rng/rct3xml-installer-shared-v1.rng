<?xml version="1.0" encoding="UTF-8"?>
<grammar datatypeLibrary="http://www.w3.org/2001/XMLSchema-datatypes"
         xmlns="http://relaxng.org/ns/structure/1.0"
         xmlns:ns="http://relaxng.org/ns/structure/1.0"
         xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">
  <define name="guid">
    <data type="string">
      <param
      name="pattern">[0-9A-Fa-f]{8}-[0-9A-Fa-f]{4}-[0-9A-Fa-f]{4}-[0-9A-Fa-f]{4}-[0-9A-Fa-f]{12}</param>
    </data>
  </define>
  <define name="guidvalue">
    <attribute name="guid">
      <ref name="guid"></ref>
    </attribute>
    <data type="string"></data>
  </define>
  <define name="version">
    <data type="string">
      <param
      name="pattern">([1-9][0-9]+|[0-9])(.[0-9]+)?[abp]?([1-9][0-9]*)?</param>
    </data>
  </define>
</grammar>